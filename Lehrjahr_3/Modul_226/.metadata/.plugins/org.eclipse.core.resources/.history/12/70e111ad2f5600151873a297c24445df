public class MusterMaster {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public void muster(int breite, int hoehe) {
		
		for(int i=0; i<hoehe;i++) {
			
			//Print pipes
			
			if(i>0) {
				System.out.println("|");
			}
			
			System.out.println(" ");
			
			//Print minus and plus
		}
	}

}
